{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 6 * * *"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "express-entry-rounds/last_draw",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}